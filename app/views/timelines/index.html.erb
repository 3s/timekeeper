<%#*<h1>Timelines#index</h1>%>

<table id="tbl">
<tr>
<th>User</th>
<th>Date</th>
<th>Subject</th>
<th>Client</th>
<th>Time</th>
</tr>
<% @timelines.each do |t|%>
<tr>
<td><%="#{t.user.email}" %></td>
<td><%="#{t.time_spend_at.strftime("%d/%m/%Y")}" %></td>
<td><%="#{t.what}" %></td>
<td><%="#{t.customer}"%></td>
<td><%="#{t.time_spend}"%></td>
</tr>
 <% end %>
 </table>